#include <iostream>
#include <math.h>
#include <sstream>

using namespace std;

void convertToBinary(int);
string NumberToString(long);

string bin = "";

int main() {
  long tenTen = pow(10, 10);
  unsigned long long sum = 0;

  convertToBinary(5);
  
  cout << bin;
}

void convertToBinary(int n)
{
  if (n / 2 != 0) {
    convertToBinary(n / 2);
  }
  bin += NumberToString(n % 2);
}

string NumberToString (long Number)
{
  ostringstream ss;
  ss << Number;
  return ss.str();
}
